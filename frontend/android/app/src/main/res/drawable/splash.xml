<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- Background color -->
    <item android:drawable="@android:color/white" />
    
    <!-- Ocean gradient background -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:startColor="#1e40af"
                android:centerColor="#3b82f6"
                android:endColor="#1e3a8a"
                android:type="linear" />
        </shape>
    </item>
    
    <!-- Logo -->
    <item android:width="200dp" android:height="200dp" android:gravity="center">
        <vector android:width="200dp" android:height="200dp"
            android:viewportWidth="200" android:viewportHeight="200">
            
            <!-- Fish body -->
            <path android:fillColor="#f97316" 
                android:pathData="M50,80 Q70,60 110,80 Q130,90 110,120 Q70,140 50,120 Q40,100 50,80 Z"/>
            
            <!-- Fish tail -->
            <path android:fillColor="#ea580c" 
                android:pathData="M40,100 L20,70 L20,130 Z"/>
            
            <!-- Fish eye (using path instead of circle) -->
            <path android:fillColor="#ffffff" android:pathData="M90,78 a12,12 0 1,0 0,24 a12,12 0 1,0 0,-24"/>
            <path android:fillColor="#000000" android:pathData="M94,80 a6,6 0 1,0 0,12 a6,6 0 1,0 0,-12"/>
            
            <!-- Fish fins -->
            <path android:fillColor="#fb923c" 
                android:pathData="M60,110 Q50,130 70,120 Z"/>
            
            <!-- Seaweed -->
            <path android:fillColor="#16a34a" 
                android:pathData="M150,200 Q160,180 150,140 Q170,100 150,60 Q140,40 150,0" 
                android:strokeWidth="8" android:fillType="nonZero"/>
            <path android:fillColor="#16a34a" 
                android:pathData="M170,200 Q180,170 170,130 Q190,90 170,50 Q160,30 170,0" 
                android:strokeWidth="6" android:fillType="nonZero"/>
            
            <!-- Bubbles (using path instead of circle) -->
            <path android:fillColor="#87ceeb" android:pathData="M70,34 a6,6 0 1,0 0,12 a6,6 0 1,0 0,-12" android:fillAlpha="0.7"/>
            <path android:fillColor="#87ceeb" android:pathData="M130,26 a4,4 0 1,0 0,8 a4,4 0 1,0 0,-8" android:fillAlpha="0.6"/>
            <path android:fillColor="#87ceeb" android:pathData="M100,45 a5,5 0 1,0 0,10 a5,5 0 1,0 0,-10" android:fillAlpha="0.8"/>
        </vector>
    </item>
    
    <!-- App title -->
    <item android:top="320dp" android:gravity="center_horizontal">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
        </shape>
    </item>
    
</layer-list>